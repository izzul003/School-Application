<%- include('partials/header') %>
<div class="container">
    <h1 class="mx-auto" style="width: 300px;">List Subjects</h1>
    <table class="table">
        <thead class="thead-light">
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Subject Name</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(e => { %>
             <tr>
                 <th scope="row"><%= e.id %> </th>
                 <td><%= e.subject_name %> </td>
             </tr>
            <% }) %>
        </tbody>
    </table>
</div>
<%- include('partials/footer') %>  
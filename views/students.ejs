<%- include('partials/header') %>
<div class="container">
    <h1 class="mx-auto" style="width: 300px;">List Students</h1>
    <table class="table">
        <thead class="thead-light">
            <tr>
                <th scope="col">ID</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
                <th scope="col">Gender</th>
                <th scope="col">Birth Date</th>
                <th scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(e => { %>
             <tr>
                 <th scope="row"><%= e.id %> </th>
                 <td><%= e.first_name %> </td>
                 <td><%= e.last_name %></td>
                 <td><%= e.email %> </td>
                 <td><%= e.gender %> </td>
                 <td><%= e.birth_date %> </td>
                 <td><p><a href="/students/<%=e.id%>/edit">Edit</a> | <a href="/students/<%=e.id%>/delete/">Delete</a></p></td>
             </tr>
            <% }) %>
        </tbody>
    </table>
</div>
<%- include('partials/footer') %>  
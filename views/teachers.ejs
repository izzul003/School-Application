<%- include('partials/header') %>
<div class="container">
    <h1 class="mx-auto" style="width: 300px;">List Teachers</h1>
    <table class="table">
        <thead class="thead-light">
            <div class="bg-dark">
            <tr>
                <th scope="col">ID</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Email</th>
                <th scope="col">Gender</th>
            </tr>
            </div>
        </thead>
        <tbody>
            <% data.forEach(e => { %>
             <tr>
                 <th scope="row"><%= e.id %> </th>
                 <td><%= e.first_name %> </td>
                 <td><%= e.last_name %></td>
                 <td><%= e.email %> </td>
                 <td><%= e.gender %> </td>
             </tr>
            <% }) %>
        </tbody>
    </table>
</div>
<%- include('partials/footer') %>  